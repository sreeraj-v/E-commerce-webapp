    <!-- Custom Styles -->
<style>
  /* Ensure Full Width and Responsiveness */
  .intro-slider-container {
    width: 100%;
    overflow: hidden;
  }

  .swiper-slide {
    display: flex;
    align-items: center; /* Vertically center content */
    justify-content: center; /* Horizontally center content */
    text-align: center;
    position: relative;
  }

  /* Center Content */
  .intro-content {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    color: white;
    z-index: 2;
    
  }

  .intro-content h3, .intro-content h1, .intro-content div, .intro-content a {
    margin: 15px 0; /* Add spacing between elements */
  }

  /* Customize Arrows */
  .swiper-button-next, .swiper-button-prev {
    color: white !important; /* Change arrow color to white */
    width: 30px; /* Reduce arrow size */
    height: 30px;
    opacity: 0.1; /* Semi-transparent arrows */
    transition: opacity 0.3s ease;  }
  .swiper-button-next:hover, .swiper-button-prev:hover {
    opacity: 0.5; /* Fully visible on hover */
  }

  /* Customize Dots */
  .swiper-pagination-bullet {
    background-color: white !important; /* Change dots to white */
    width: 10px; /* Dot size */
    height: 10px;
    opacity: 0.7;
  }
  .swiper-pagination-bullet-active {
    opacity: 1; /* Active dot fully visible */
  }

  /* Responsive Design */
  @media (max-width: 768px) {
    .swiper-slide {
      height: 70vh; /* Adjust height for smaller screens */
    }
    .swiper-button-next, .swiper-button-prev {
      width: 25px; /* Smaller arrows for mobile */
      height: 25px;
    }
    .swiper-pagination-bullet {
      width: 8px; /* Smaller dots for mobile */
      height: 8px;
    }
  }
</style>

